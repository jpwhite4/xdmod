        "type": "integer",
        "nullable": false
      },
      {
        "name": "group_by_id",
        "type": "integer",
        "nullable": false
      },
      {
        "name": "realm_id",
        "type": "integer",
        "nullable": false
      },
      {
        "name": "statistic_id",
        "type": "integer",
        "nullable": false
      },
      {
        "name": "visible",
